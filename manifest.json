{
	"manifest_version": 3,
	"name": "Search Shortcut",
	"description": "Activate search boxes with a keyboard shortcut!",
	"version": "2.0",
	"icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
	"permissions": [
		"activeTab",
		"storage",
		"scripting"
	],
	"action": {},
	"background": {
		"service_worker": "background.js"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Alt+S"
			}
		}
	},
	"options_ui": {
        "page": "options/options.html",
        "open_in_tab": false
    }
}